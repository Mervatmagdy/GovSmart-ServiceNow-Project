        <template><![CDATA[<div class="service-details-container">
  <div class="header-tools">
    <div class="breadcrumbs">Digital Services > {{data.category}} > <strong>{{data.title}}</strong></div>
  </div>

  <h2 class="service-title">{{data.title}}</h2>

  <div class="details-grid">
    <div class="details-column">
      <div class="info-card">
        <div class="card-header"><i class="fa fa-file-text-o"></i> Service Description</div>
        <div class="card-body">{{data.description}}</div>
      </div>
      <div class="info-card">
        <div class="card-header"><i class="fa fa-gavel"></i> Terms and Conditions</div>
        <div class="card-body">
          <ul><li ng-repeat="term in data.terms">{{term}}</li></ul>
        </div>
      </div>
    </div>

    <div class="details-column">
      <div class="info-card">
        <div class="card-header"><i class="fa fa-list-ul"></i> Required Documents</div>
        <div class="card-body">
          <ul><li ng-repeat="doc in data.documents">{{doc}}</li></ul>
        </div>
      </div>
    </div>
  </div>

  <div class="action-footer">
    <label class="agreement">
      <input type="checkbox" ng-model="c.agreed"> I agree to the terms and conditions
    </label>
    <br>
    <button class="btn-continue" ng-disabled="!c.agreed" ng-click="c.nextStep()">
      Continue to fill data <i class="fa fa-arrow-right"></i>
    </button>
  </div>
</div>]]></template>
