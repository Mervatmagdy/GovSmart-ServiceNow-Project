        <script><![CDATA[(function() {
  var serviceId = $sp.getParameter('service_id');
  
  if (serviceId == 'family_extract') {
    data.title = "Family Registration Extract";
    data.category = "Civil Status";
    data.description = "Official extract of family registration records.";
    data.documents = ["Original ID Card", "Proof of kinship"];
    data.terms = ["First-degree relative only", "Updated civil registry data"];
    data.similar = ["Birth Certificate", "Marriage Certificate"];
  } 
  else if (serviceId == 'renew_id') {
    data.title = "Renew National ID Card";
    data.category = "Civil Status";
    data.description = "Renewal of National ID card before or after expiry.";
    data.documents = ["Old ID card", "Proof of employment/student status"];
    data.terms = ["Personal attendance required", "Renewal every 7 years"];
    data.similar = ["Lost ID replacement", "Change of Address"];
  } 
  else if (serviceId == 'renew_license') {
    data.title = "Renew Driver's License";
    data.category = "Traffic Services";
    data.description = "Electronic renewal of driving licenses.";
    data.documents = ["Expired license", "Medical certificate", "Criminal record"];
    data.terms = ["No outstanding traffic fines", "Valid for 10 years"];
    data.similar = ["Vehicle License Renewal", "International Permit"];
  } 
  else if (serviceId == 'id_application') {
    data.title = "National ID Card Application";
    data.category = "Civil Status";
    data.description = "First-time issuance for citizens reaching 15 years old.";
    data.documents = ["Automated Birth certificate", "Parent ID copy"];
    data.terms = ["Minimum age 15", "Witnesses might be required"];
    data.similar = ["ID Renewal", "Birth Certificate Request"];
  }
})();]]></script>
