        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Set Service Type Prefix</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue == '') {
        return;
    }

    var prefix = '';

    switch (newValue) {
        case 'family_registration_extract':
            prefix = 'FRE';
            break;

        case 'renew_the_national_id_card':
            prefix = 'NID-R';
            break;

        case 'renew_your_driver_s_license':
            prefix = 'DL-R';
            break;

        case 'national_id_card_application':
            prefix = 'NID-A';
            break;

        default:
            prefix = '';
    }

    g_form.setValue('service_type_prefix', prefix);
}
]]></script>
